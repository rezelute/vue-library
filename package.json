{
   "name": "@rezelute/vue-shared-lib",
   "version": "1.0.2",
   "private": true,
   "type": "module",
   "module": "./dist/index.js",
   "types": "./dist/index.d.ts",
   "exports": {
      ".": {
         "import": "./dist/index.js",
         "types": "./dist/index.d.ts"
      },
      "./components": {
         "import": "./dist/components/index.js",
         "types": "./dist/components/index.d.ts"
      },
      "./routing": {
         "import": "./dist/router/index.js",
         "types": "./dist/router/index.d.ts"
      },
      "./stores": {
         "import": "./dist/stores/index.js",
         "types": "./dist/stores/index.d.ts"
      },
      "./composables": {
         "import": "./dist/composables/index.js",
         "types": "./dist/composables/index.d.ts"
      },
      "./utils": {
         "import": "./dist/utils/index.js",
         "types": "./dist/utils/index.d.ts"
      },
      "./types": {
         "import": "./dist/types/index.js",
         "types": "./dist/types/index.d.ts"
      },
      "./tailwind-config.css": {
         "default": "./dist/tailwind-config.css"
      },
      "./vue-shared-lib.css": {
         "default": "./dist/vue-shared-lib.css"
      }
   },
   "files": [
      "dist"
   ],
   "prepare": "vite build",
   "scripts": {
      "dev": "vite",
      "build": "run-p tscheck \"build-only {@}\" --",
      "build-w-types": "run-s generate-types build-only",
      "generate-types": "vue-tsc --build tsconfig.build.json",
      "build-only": "vite build",
      "build-watch": "vite build --watch",
      "build-pack": "npm run build && npm pack",
      "preview": "vite preview",
      "--TESTING--": "",
      "test:unit": "vitest",
      "--PROJECT--": "",
      "tscheck": "vue-tsc --build",
      "lint": "eslint . --fix",
      "format": "prettier --write src/",
      "stylelint": "stylelint \"**/*.scss\" \"**/*.vue\"",
      "stylelint-fix": "stylelint \"**/*.scss\" \"**/*.vue\" --fix"
   },
   "dependencies": {
      "supertokens-web-js": "^0.14.0"
   },
   "peerDependencies": {
      "@primeuix/themes": "^1.0.0",
      "@tailwindcss/vite": "^4.0.0",
      "pinia": "^3.0.0",
      "primeicons": "^7.0.0",
      "primevue": "^4.0.0",
      "tailwindcss": "^4.0.0",
      "tailwindcss-primeui": "^0.5.1",
      "vee-validate": "^4.0.0",
      "vue": "^3.0.0",
      "vue-router": "^4.0.0",
      "zod": "^4.1.5"
   },
   "bundleDependencies": [
      "supertokens-web-js"
   ],
   "devDependencies": {
      "@tsconfig/node22": "^22.0.0",
      "@types/jsdom": "^21.1.7",
      "@types/node": "^22.13.10",
      "@vitejs/plugin-vue": "^5.2.1",
      "@vitest/eslint-plugin": "^1.1.37",
      "@vue/eslint-config-prettier": "^10.1.0",
      "@vue/eslint-config-typescript": "^14.5.0",
      "@vue/test-utils": "^2.4.6",
      "@vue/tsconfig": "^0.7.0",
      "dotenv": "^16.4.7",
      "eslint": "^9.22.0",
      "eslint-plugin-simple-import-sort": "^12.1.1",
      "eslint-plugin-vue": "^10.0.0",
      "jiti": "^2.4.2",
      "jsdom": "^26.0.0",
      "npm-run-all2": "^7.0.2",
      "prettier": "^3.5.3",
      "rollup-plugin-copy": "^3.5.0",
      "sass": "^1.85.1",
      "stylelint": "^16.14.1",
      "stylelint-config-recess-order": "^6.0.0",
      "stylelint-config-standard": "^37.0.0",
      "stylelint-config-standard-scss": "^14.0.0",
      "stylelint-config-standard-vue": "^1.0.0",
      "tailwindcss": "^4.0.14",
      "typescript": "^5.8.2",
      "vite": "^6.2.1",
      "vite-plugin-dts": "^4.5.3",
      "vitest": "^3.0.8",
      "vue-eslint-parser": "^10.1.1",
      "vue-tsc": "^2.2.8",
      "zod": "^4.1.5"
   }
}
